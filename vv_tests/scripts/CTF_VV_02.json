                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "CFE_EVS_LONG_EVENT_MSG_MID",
                        "args": [
                            {
                                "compare": "strneq",
                                "variable": "Payload.PacketID.AppName",
                                "value": "SCHNO"
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "CFE_EVS_LONG_EVENT_MSG_MID",
                        "args": [
                            {
                                "compare": "regex",
                                "variable": "Payload.PacketID.AppName",
                                "value": "S[CK]H"
                            }
                        ]
                    },
                    "wait": 1
                }
            ]
        }
    ]
