    "tests": [
        {
            "case_number": "CTF Test Case 01",
            "description": "Integer numbers comparison of with operators '<', '>', '<=', '>=', '==', '!=' ",
            "instructions": [
                {
                        "instruction": "SendCfsCommand",
                        "data": {
                                "target": "",
                                "mid": "TO_CMD_MID",
                                "cc": "TO_RESET_CC",
                                "args": {}
                        },
                        "wait": 0
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "<",
                                "variable": "usCmdCnt",
                                "value": 3
                            },
                            {
                                "compare": ">",
                                "variable": "usCmdCnt",
                                "value": 0
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": ">=",
                                "variable": "usCmdCnt",
                                "value": 1
                            },
                            {
                                "compare": "<=",
                                "variable": "usCmdCnt",
                                "value": 1
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "==",
                                "variable": "usCmdCnt",
                                "value": 1
                            },
                            {
                                "compare": "!=",
                                "variable": "usCmdCnt",
                                "value": 2
                            }
                        ]
                    },
                    "wait": 1
                }
            ]
        }
    ]
