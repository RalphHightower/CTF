    "functions": {},
    "tests": [
        {
            "case_number": "CFE-6-7-Plugin-Test-001",
            "description": "Start CFS, Send TO NOOP command",
            "instructions": [
                {
                    "instruction": "StartCfs",
                    "wait": 1,
                    "data": {
                        "target": ""
                    }
                },
                {
                    "instruction": "EnableCfsOutput",
                    "wait": 1,
                    "data": {
                        "target": ""
                    }
                },
                {
                    "function": "SendCheckCfsTimeNoopCmd",
                    "wait": 5,
                    "params": {
                        "expectedCmdCnt": 1,
                        "expectedErrCnt": 0
                    }
                },
                {
                    "function": "SendCheckCfsTimeResetCmd",
                    "wait": 1,
                    "params": {
                        "expectedCmdCnt": 0,
                        "expectedErrCnt": 0
                    }
                },
                {
                    "function": "SendCheckCfsTimeSubDelayCmd",
                    "wait": 1,
                    "comment": "Off-nominal Case. Command gets accepted, but data is invalid",
                    "params": {
                        "expectedCmdCnt": 0,
                        "expectedErrCnt": 1,
                        "sec": 1,
                        "ms": 100
                    }
                },
                {
                    "function": "SendCheckCfsTimeSetStateCmd",
                    "wait": 1,
                    "params": {
                        "expectedCmdCnt": 1,
                        "expectedErrCnt": 1,
                        "state": 0
                    }
                },
                {
                    "function": "SendCheckCfsTimeSetMetCmd",
                    "wait": 1,
                    "params": {
                        "expectedCmdCnt": 2,
                        "expectedErrCnt": 1,
                        "sec": 0,
                        "ms": 100
                    }
                }
            ]
        }
    ]
}