{
    "test_number": "Basic-CFE-6-7-Cfs-Control-Flow-Plugin-Test",
    "test_name": "Basic CFE-6-7 CFS Control-Flow Plugin Test",
    "requirements": {
        "MyRequirement": "N/A"
    },
    "description": "Basic CTF Example Script Showing Simple Control Flow (Looping)",
    "owner": "CTF",
    "test_setup": "Script will start CFE-6-7, execute loop instruction test and close CFE-6-7",
    "ctf_options": {
        "verify_timeout": 4
    },
    "import": {},
    "tests": [
        {
            "case_number": "CFE-6-7-Control-Flow-Test-1",
            "description": "Start CFS, Send TO NOOP command, And Conduct Two Loops",
            "instructions": [
                {
                    "instruction": "StartCfs",
                    "data": {
                        "target": ""
                    },
                    "wait": 1
                },
                {
                    "instruction": "EnableCfsOutput",
                    "data": {
                        "target": ""
                    },

                    "wait": 1,
                    "description": "Need this to enable the telemetry thread. Enable Output counts as 1 TO cmd"
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_var",
                        "operator": "=",
                        "value": 0
                    },
                    "wait": 1
                },
                {
                    "instruction": "BeginLoop",
                    "data": {
                        "label": "LOOP_1",
                        "conditions": [
                            {"variable": "my_var", "compare": "<", "value": 3}
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_var",
                        "operator": "+",
                        "value": 1
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "EndLoop",
                    "data": {
                        "label": "LOOP_1"
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_var",
                        "operator": "=",
                        "value": 0
                    },
                    "wait": 1
                },
                {
                    "instruction": "BeginLoop",
                    "data": {
                        "label": "LOOP_2",
                         "conditions": [
                            {"variable": "my_var", "compare": "<", "value": 3}
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_var",
                        "operator": "+",
                        "value": 1
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "EndLoop",
                    "data": {
                        "label": "LOOP_2"
                    },
                    "wait": 1
                }
            ]
        },
        {
            "case_number": "CFE-6-7-Control-Flow-Test-2",
            "description": "Send TO NOOP command, Conduct Nested Loops",
            "instructions": [
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_out_var",
                        "operator": "=",
                        "value": 0
                    },
                    "wait": 1
                },
                {
                    "instruction": "BeginLoop",
                    "data": {
                        "label": "OUTER_LOOP",
                         "conditions": [
                            {"variable": "my_out_var", "compare": "<", "value": 3}
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_out_var",
                        "operator": "+",
                        "value": 1
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_inner_var",
                        "operator": "=",
                        "value": 0
                    },
                    "wait": 1
                },
                {
                    "instruction": "BeginLoop",
                    "data": {
                        "label": "INNER_LOOP",
                        "conditions": [
                            {"variable": "my_inner_var", "compare": "<", "value": 2}
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SetUserVariable",
                    "data": {
                        "variable_name": "my_inner_var",
                        "operator": "+",
                        "value": 1
                    },
                    "wait": 1
                },
                {
                    "instruction": "EndLoop",
                    "data": {
                        "label": "INNER_LOOP"
                    },
                    "wait": 1
                },
                {
                    "instruction": "EndLoop",
                    "data": {
                        "label": "OUTER_LOOP"
                    },
                    "wait": 1
                },
                {
                    "instruction": "ShutdownCfs",
                    "data": {
                        "target": ""
                    },
                    "wait": 1
                }
            ]
        }
    ]
}
