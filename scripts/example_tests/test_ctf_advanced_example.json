{
    "test_number": "Advanced-CFE-6-7-Cfs-Plugin-Test",
    "test_name": "Advanced CFE-6-7 CFS Plugin Test",
    "requirements": {
        "MyRequirement": "N/A"
    },
    "description": "Testing CFE-6-7 Plugin",
    "owner": "CTF",
    "test_setup": "Advanced CTF Example Script Showing Commands/Telemetry Verification with variable tolerances, event checking, and function usage",
    "ctf_options": {
        "verify_timeout": 4
    },
    "import": {},
    "functions": {
        "Increment Event": {
            "description": "No description",
            "varlist": [
                "expectedCmdCnt",
                "expectedErrCnt"
            ],
            "instructions": [
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "variable": "usCmdCnt",
                                "value": [
                                    "expectedCmdCnt"
                                ],
                                "compare": "=="
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "variable": "usCmdErrCnt",
                                "value": [
                                    "expectedErrCnt"
                                ],
                                "compare": "=="
                            }
                        ]
                    },
                    "wait": 1
                }
            ]
        }
    },
    "tests": [
        {
            "case_number": "CFE-6-7-Plugin-Test-001",
            "description": "Start CFS, Send TO NOOP command",
            "instructions": [
                {
                    "instruction": "BuildCfs",
                    "data": {
                        "target": ""
                    },
                    "wait": 1
                },
                {
                    "instruction": "StartCfs",
                    "data": {
                        "target": ""
                    },
                    "wait": 1
                },
                {
                    "instruction": "EnableCfsOutput",
                    "data": {
                        "target": ""
                    },
                    "wait": 5
                },
                {
                    "instruction": "CheckEvent",
                    "data": {
                        "app": "TO",
                        "id": "3",
                        "msg": "TO - ENABLE_OUTPUT cmd succesful for  routeMask:0x00000001",
                        "msg_args": "",
                        "target": ""
                    },

                    "wait": 0,
                    "description": "Need this to enable the telemetry thread. Enable Output counts as 1 TO cmd"
                },
                {
                    "description": "This will format the message with msg_args and then check for a regex match",
                    "instruction": "CheckEvent",
                    "data": {
                        "app": "TO",
                        "id": "3",
                        "msg": "%s - %s cmd succesful for  routeMask:0x[0-9]+",
                        "is_regex": true,
                        "msg_args": "('TO', 'ENABLE_OUTPUT')",
                        "target": ""
                    },

                    "wait": 0
                },
                {
                    "description": "This will format the message with msg_args and then check for a regex match",
                    "instruction": "CheckEvent",
                    "data": {
                        "app": "TO",
                        "id": "3",
                        "msg": "%s - %s cmd succesful for  routeMask:0x[0-9]+",
                        "is_regex": true,
                        "msg_args": "('TO', 'ENABLE_OUTPUT')",
                        "target": ""
                    },

                    "wait": 0
                },
                {
                    "description": "This will format the message with msg_args and then check for a regex match",
                    "instruction": "CheckEvent",
                    "data": {
                        "app": "TO",
                        "id": "3",
                        "msg": "%s - %s cmd succesful for  routeMask:0x[0-9]+",
                        "is_regex": true,
                        "msg_args": "('TO', 'ENABLE_OUTPUT')",
                        "target": ""
                    },

                    "wait": 0
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "==",
                                "variable": "usCmdErrCnt",
                                "value": [
                                    0.1
                                ],
                                "tolerance_minus": 0.1
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendInvalidLengthCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_NOOP_CC",
                        "payload_length": 10,
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "==",
                                "variable": "usCmdErrCnt",
                                "value": [
                                    1
                                ]
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "function": "Increment Event",
                    "params": {
                        "expectedCmdCnt": 2,
                        "expectedErrCnt": 1
                    },
                    "wait": 1
                },
                {
                    "function": "Increment Event",
                    "params": {
                        "expectedCmdCnt": 3,
                        "expectedErrCnt": 1
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "==",
                                "variable": "usCmdErrCnt",
                                "value": [
                                    1
                                ]
                            },
                            {
                                "compare": "==",
                                "variable": "usCmdCnt",
                                "value": [
                                    2.95
                                ],
                                "tolerance_plus": 0.1
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "TO_CMD_MID",
                        "cc": "TO_RESET_CC",
                        "args": {}
                    },
                    "wait": 1
                },
                {
                    "instruction": "CheckTlmValue",
                    "data": {
                        "target": "",
                        "mid": "TO_HK_TLM_MID",
                        "args": [
                            {
                                "compare": "==",
                                "variable": "usCmdCnt",
                                "value": [
                                    0
                                ]
                            }
                        ]
                    },
                    "wait": 1
                },
                {
                    "description": "Demonstrate the use of header parameters (has no effect in default CCSDS implementation)",
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "CFE_ES_CMD_MID",
                        "cc": "CFE_ES_SHELL_CC",
                        "args": {
                            "Payload": {
                                "CmdString": "hostname",
                                "OutputFilename": "/cf/test_output.txt"
                            }
                        },
                        "header": {
                            "field_name": "value"
                        }
                    },
                    "wait": 1
                },
                {
                    "description": "Demonstrate the use of macros in command args",
                    "instruction": "SendCfsCommand",
                    "data": {
                        "target": "",
                        "mid": "SCH_CMD_MID",
                        "cc": "SCH_ENABLE_CC",
                        "args": {
                            "SlotNumber": "#SCH_TOTAL_SLOTS",
                            "EntryNumber": "#SCH_ENTRIES_PER_SLOT"
                        }
                    },
                    "wait": 1
                },
                {
                    "instruction": "ArchiveCfsFiles",
                    "data": {
                        "target": "",
                        "source_path": "/root/sample_cfs_workspace/build/exe/lx1/cf/"
                    },
                    "wait": 1
                },
                {
                    "instruction": "ShutdownCfs",
                    "data": {
                        "target": ""
                    },
                    "wait": 1
                }
            ]
        }
    ]
}
